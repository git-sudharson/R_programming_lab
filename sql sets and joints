install.packages("sqldf")
install.packages("readr")
library(sqldf)
library(readr)
table1<-read_csv("E:/R/RProgram/table1.csv")
table2<-read_csv("E:/R/RProgram/table2.csv")
View(table1)
View(table2)
sqldf("SELECT * FROM table1 UNION SELECT * FROM table2")
sqldf("SELECT * FROM table1 UNION ALL SELECT * FROM table2")
sqldf("SELECT * FROM table1 INTERSECT SELECT * FROM table2")
sqldf("SELECT * FROM table1 EXCEPT SELECT * FROM table2")

table3 <- read_cse("D:\\table3.cse")
view(table3)
sqldf("SELECT * FROM table1 INNER JOIN table3 ON table1.Id=table3.Id")
sqldf("SELECT * FROM table1 FULL OUTER JOIN table3 ON table1.Id=table3.Id")
sqldf("SELECT * FROM table1 LEFT OUTER JOIN table3 ON table1.Id=table3.Id")
sqldf("SELECT * FROM table1 RIGHT OUTER JOIN table3 ON table1.Id=table3.Id")
