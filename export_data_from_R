install.packages("openxlsx")
library(openxlsx)

s.no <- seq(1,3, by=1)
fruits <- c("apple", "mango", "orange")
shop1 <- c(12,14,16)
shop2 <- c(22,23,44)
shop3 <- c(12,15,24)
shop4 <- c(66,26,33)

rate <- data.frame(fruits,shop1,shop2,shop3,shop4)
rate 

write.xlsx(rate, file="D:/sudharson/exportdata.xlsx")
